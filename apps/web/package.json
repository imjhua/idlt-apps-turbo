{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "========================================== [ENV]": "",
    "env:dev": "cp env/.env.local .env",
    "env:prod": "cp env/.env.production .env",
    "========================================== [DEV]": "",
    "dev": "pnpm run env:dev && next dev --turbopack --port 8080 -H idlt.localhost",
    "build:dev": "pnpm run env:dev && next build",
    "========================================== [BUILD]": "",
    "build": "pnpm lint && pnpm run env:prod && next build",
    "========================================== [START]": "",
    "start": "next start -p 8080",
    "========================================== [ETC]": "",
    "test": "jest",
    "lint": "next lint",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.2",
    "@repo/request": "workspace:*",
    "@repo/ui": "workspace:*",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@tanstack/react-query": "^5.74.7",
    "@tanstack/react-query-devtools": "^5.74.7",
    "@tanstack/react-table": "^8.21.2",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.475.0",
    "next": "^15.1.6",
    "nuqs": "^2.4.1",
    "react": "^18.3.1",
    "react-day-picker": "^9.5.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "sonner": "^2.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^29.5.14",
    "@types/node": "^22",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "jest": "^29.7.0",
    "kakao.maps.d.ts": "^0.1.40",
    "local-ssl-proxy": "^2.0.5",
    "ts-jest": "^29.3.4",
    "typescript": "5.7.3",
    "yaml-loader": "^0.8.1"
  }
